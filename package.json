{
  "name": "m3u-tv",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "EXPO_TV=1 expo start",
    "android": "EXPO_TV=1 expo run:android",
    "ios": "EXPO_TV=1 expo run:ios",
    "web": "expo start --web",
    "prebuild": "EXPO_TV=1 expo prebuild --clean",
    "clean": "rm -rf node_modules android ios",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nessprim/planby-native-pro": "1.0.0-beta.86",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/drawer": "^7.4.0",
    "@react-navigation/native": "^7.1.1",
    "@react-navigation/native-stack": "^7.3.2",
    "expo": "^54.0.20",
    "expo-build-properties": "~1.0.9",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "npm:react-native-tvos@0.81-stable",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1",
    "react-tv-space-navigation": "^6.0.0-beta1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-tvos/config-tv": "^0.1.4",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  }
}
